{
    "parser": "babel-eslint",
    "rules": {
        "strict": [2, "global"],
        "quotes": [2, "single", "avoid-escape"],
        "no-use-before-define": [2, "nofunc"],
        "no-underscore-dangle": [0]
    },
    "env": {
        "node": true,
        "browser": true
    },
    "globals": {
        "describe": true,
        "beforeEach": true,
        "it": true,
        "inject": true,
        "expect": true
    }
}
